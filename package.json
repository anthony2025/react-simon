{
  "name": "react-simon",
  "version": "1.0.0",
  "description": "Simon game made in React",
  "homepage": "https://react-simon-game.firebaseapp.com/",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "deploy": "yarn build && firebase deploy",
    "analyze": "yarn build && source-map-explorer build/static/js/main.*",
    "format": "prettier --single-quote --no-bracket-spacing --no-semi --write 'src/**/*.js'"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "raven-js": "^3.17.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.4.2",
    "styled-components": "^2.1.2",
    "web-animations-js": "^2.3.1"
  },
  "devDependencies": {
    "firebase-tools": "^3.9.2",
    "prettier": "^1.5.3",
    "react-scripts": "1.0.10",
    "source-map-explorer": "^1.4.0"
  },
  "author": "Anthony Ascencio",
  "license": "CC0",
  "repository": "https://github.com/anthony2025/react-simon-game"
}
